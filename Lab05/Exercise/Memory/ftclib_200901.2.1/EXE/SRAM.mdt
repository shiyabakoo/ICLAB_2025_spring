tfgDefineMem -module "SRAM" -array_name "Memory" -banks * -addr_range 127 0
tfgDefineMemWrite -module "SRAM" -array_name "Memory" -clk "@(posedge CK)" -cond "CS && ~WEB" -addr "`<->A" -data "`<->DI"
tfgDefineMemRead -module "SRAM" -array_name "Memory" -clk "@(posedge CK)" -cond "CS && WEB" -addr "`<->A" -noe "OE" -out "`<->DO"
